# vim: set ts=8 sts=2 sw=2 tw=99 et ft=python: 
import os, sys

builder.SetBuildFolder('/')
builder.AddFolder('includes')

argv = [
  sys.executable,
  os.path.join(builder.sourcePath, 'tools', 'auto_version.py'),
  '--output',
  os.path.join(builder.buildPath, 'includes'),
]
outputs = [
  os.path.join(builder.buildFolder, 'includes', 'generated_version.h'),
]

input = [
    os.path.join(builder.sourcePath, 'tools', 'auto_version.py'),
]

output_nodes = builder.AddCommand(
  inputs=input,
  argv=argv,
  outputs=outputs
)

rvalue = output_nodes